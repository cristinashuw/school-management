<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="student_actions_id" model="ir.actions.act_window">
        <field name="name">Student</field>
        <field name="res_model">student</field>
    </record>

    <record id="student_actions_tree_view_id" model="ir.actions.act_window.view">
        <field name="sequence" eval="1"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="student.student_tree_view_id"/>
        <field name="act_window_id" ref="student.student_actions_id"/>
    </record>
</odoo>